module sample
import org.eclipse.xtext.xtext.generator.*
import org.eclipse.xtext.xtext.generator.model.project.*
import edu.ustb.sei.mde.mwe2.*
var rootPath = "."
Workflow {
  component = edu.ustb.sei.mde.mwe2.EcoreGenerator {
    configuration = org.eclipse.xtext.xtext.generator.DefaultGeneratorModule {
      project = org.eclipse.xtext.xtext.generator.model.project.StandardProjectConfig {
        baseName = "."
        rootPath = rootPath
        createEclipseMetaData = false
      }
      code = org.eclipse.xtext.xtext.generator.CodeConfig {
        encoding = "UTF-8"
        lineDelimiter = "\r\n"
        fileHeader = "/*\n * generated by Enhanced EcoreGen\n */"
        preferXtendStubs = false
      }
    }
    language = edu.ustb.sei.mde.mwe2.EcoreLanguage {

      ecoreFile = "model/fleet5.ecore"

      basePackage = "edu"
      fileExtensions = "library"
      suppressInterfaces = true
      llmConfig = "conf.properties"

      classpathFile = "target/classpath.txt"

    }
    languageModule = 'edu.ustb.sei.mde.eecg.llm.languageModule.DefaultEnhancedEcoreGenModule'
  }

}